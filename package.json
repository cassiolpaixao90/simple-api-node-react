{
  "name": "simple-api-node-react",
  "version": "0.0.0",
  "scripts": {
    "prestart": "babel-node ./src/utils/utils.js",
    "start": "npm-run-all --parallel open:src",
    "open:src": "nodemon --exec npm run babel-node -- ./src/clusters.js",
    "babel-node": "babel-node --presets=env",
    "security-check": "nsp check",
    "share": "lt --port 3000",
    "localtunnel": "npm-run-all --parallel open:src localtunnel"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "bluebird": "3.3.4",
    "body-parser": "^1.18.3",
    "connect-mongo": "1.3.2",
    "es6-promisify": "^5.0.0",
    "express": "^4.16.2",
    "express-validator": "^5.2.0",
    "mongodb": "2.1.18",
    "mongoose": "^5.1.2",
    "morgan": "^1.6.1",
    "lodash": "latest",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "nodemon": "^1.17.3"
  },
  "devDependencies": {
    "chalk": "1.1.3"
  }
